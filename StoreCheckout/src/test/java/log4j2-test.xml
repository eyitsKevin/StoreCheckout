<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="Sale" packages="">
	<Appenders>
    	<Console name="STDIO" target="SYSTEM_OUT">
    		<PatternLayout>
	        	<Pattern>%d %p %c [%t] %m%n</Pattern>
	      	</PatternLayout>
    	</Console>
    	
    	<File name="Migration" fileName="logs/Migration.log">
	      	<PatternLayout>
	        	<Pattern>%d %p %c [%t] %m%n</Pattern>
	      	</PatternLayout>
    	</File>
    	
	</Appenders>
	<Loggers>
		<Root level="debug">
	      <AppenderRef ref="STDIO"/>
	    </Root>
	    <Logger name = "migration" level = "error" additivity = "false">
	    	<AppenderRef ref= "Migration"/>
	    </Logger>
	</Loggers>
</Configuration>